<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.controller.HomeController">
    <padding>
        <Insets top="50" right="50" bottom="50" left="50"/>
    </padding>

    <Button text="Empleados" fx:id="empleadosButton" onAction="#handleEmpleados"/>
    <Button text="Usuarios" fx:id="usuariosButton" onAction="#handleUsuarios"/>
    <Button text="Salir" fx:id="salirButton" onAction="#handleSalir"/>
</VBox>
